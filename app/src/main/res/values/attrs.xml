<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleBorderTextView">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="borderColor" format="reference|color" />
        <attr name="thickness" format="dimension" />
        <attr name="android:background" />
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="GraduallyFillBorderTextView">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="gfb_progress" format="integer" />
        <attr name="gfb_borderColor" format="reference|color" />
        <attr name="gfb_thickness" format="dimension" />
        <attr name="android:text" />
        <attr name="android:textColor" />
    </declare-styleable>

    <declare-styleable name="ColorChangingCircle">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="ccc_color" format="reference|color" />
        <attr name="ccc_touch_color" format="reference|color" />
        <attr name="ccc_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IOSRadio">
        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="ios_radio_first" format="string" />
        <attr name="ios_radio_second" format="string" />
        <attr name="ios_radio_third" format="string" />
        <attr name="ios_radio_fourth" format="string" />
    </declare-styleable>
</resources>